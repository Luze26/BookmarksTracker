{
  "name": "Bookmarks Tracker",
  "version": "0.5",
  "background": { "scripts": ["background.js"] },
  "permissions": [
    "tabs", "<all_urls>", "storage", "bookmarks"
  ],
  "browser_action": {
    "name": "Msq",
    "icons": ["icon.png"],
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}